{
  "cli": {
    "version": ">= 3.13.3"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_FIREBASE_API_KEY": {
          "secretName": "EXPO_PUBLIC_FIREBASE_API_KEY"
        },
        "EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN": {
          "secretName": "EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN"
        },
        "EXPO_PUBLIC_FIREBASE_PROJECT_ID": {
          "secretName": "EXPO_PUBLIC_FIREBASE_PROJECT_ID"
        },
        "EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET": {
          "secretName": "EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET"
        },
        "EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID": {
          "secretName": "EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID"
        },
        "EXPO_PUBLIC_FIREBASE_APP_ID": {
          "secretName": "EXPO_PUBLIC_FIREBASE_APP_ID"
        },
        "EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID": {
          "secretName": "EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID"
        }
      }
    }
  },
  "submit": {
